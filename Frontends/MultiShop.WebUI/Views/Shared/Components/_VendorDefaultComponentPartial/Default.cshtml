@model List<ResultBrandDto>
<div class="container-fluid py-5">
    <div class="row px-xl-5">
        <div class="col">
            <div class="owl-carousel vendor-carousel" style="height:120;width:200">
                @foreach(var item in Model){
                    <div class="bg-light p-4" style="height:120;width:200">
                        <img src="@item.ImageUrl" alt="">
                    </div>
                }
            </div>
        </div>
    </div>
</div>